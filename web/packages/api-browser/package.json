{
    "name": "@goauthentik/api-browser",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "node ./build.mjs",
        "build:bundle": "rollup -c ./rollup.config.mjs",
        "build:compile": "node ./build.mjs",
        "build:types": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.types.json",
        "clean": "rimraf dist",
        "fix:lint": "eslint . --max-warnings 0 --fix ./src/",
        "fix:prettier": "prettier --write ./src/**/* ./package.json",
        "fix": "npm-run-all --sequential fix:lint fix:prettier",
        "lint:lint": "eslint . --max-warnings 0 ./src/",
        "lint:prettier": "prettier ./src/**/* ./package.json",
        "lint:typecheck": "tsc --noEmit -p .",
        "lint:spelling": "codespell -D - -D ../../../.github/codespell-dictionary.txt -I ../../../.github/codespell-words.txt ./src -s",
        "lint": "npm-run-all --parallel lint:lint lint:prettier lint:typecheck lint:spelling",
        "precommit": "npm-run-all  --parallel lint:typecheck lint:spelling --sequential fix:lint fix:prettier",
        "fix:types": "typesync"
    },
    "exports": {
        "./*": "./dist/*"
    },
    "files": [
        "./dist/**/*"
    ],
    "dependencies": {
        "@goauthentik/api": "^2023.10.6-1705263684",
        "@goauthentik/common": "*",
        "@goauthentik/elements": "*",
        "@patternfly/patternfly": "^4.224.2",
        "rapidoc": "^9.3.4"
    },
    "peerDependencies": {
        "lit": "^2.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.23.7",
        "@babel/plugin-transform-private-methods": "^7.23.3",
        "@babel/plugin-transform-private-property-in-object": "^7.23.4",
        "@babel/plugin-transform-runtime": "^7.23.7",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-typescript": "^7.23.3",
        "@lit/localize-tools": "^0.7.1",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@storybook/addon-essentials": "^7.6.9",
        "@storybook/addon-links": "^7.6.9",
        "@storybook/api": "^7.6.9",
        "@storybook/blocks": "^7.6.4",
        "@storybook/manager-api": "^7.6.9",
        "@storybook/web-components": "^7.6.9",
        "@storybook/web-components-vite": "^7.6.9",
        "@types/eslint": "~8.56.2",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-tsconfig-paths": "^1.0.3",
        "esbuild": "^0.19.10",
        "eslint": "^8.56.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-custom-elements": "0.0.8",
        "glob": "^10.3.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.1.1",
        "pseudolocale": "^2.0.0",
        "rimraf": "^5.0.5",
        "storybook": "^7.6.9",
        "storybook-addon-mock": "^4.3.0",
        "ts-lit-plugin": "^2.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "typesync": "^0.12.1"
    }
}
