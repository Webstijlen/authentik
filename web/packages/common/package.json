{
    "name": "@goauthentik/common",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "locales:extract": "lit-localize extract",
        "locales:build": "lit-localize build",
        "build": "npm-run-all --sequential locales:build --parallel build:styles build:compile build:types",
        "build:compile": "node ./build.mjs",
        "build:styles": "mkdir -p ./dist/styles && cp ./src/styles/*.css ./dist/styles",
        "build:types": "tsc --declaration --emitDeclarationOnly -p .",
        "clean": "rimraf dist",
        "fix:lint": "eslint . --max-warnings 0 --fix ./src/ ./build.mjs",
        "fix:prettier": "prettier --write ./src/* ./src/**/* ./build.mjs ./tsconfig.build.json ./tsconfig.json ./package.json",
        "fix": "npm-run-all --sequential fix:lint fix:prettier",
        "lint:lint": "eslint . --max-warnings 0 ./src/ ./build.mjs",
        "lint:prettier": "prettier ./src/* ./src/**/* ./build.mjs ./tsconfig.build.json ./tsconfig.json ./package.json",
        "lint:typecheck": "tsc --noEmit -p .",
        "lint:spelling": "codespell -D - ./src ./build.mjs ./package.json ./tsconfig.json ./tsconfig.build.json",
        "lint": "npm-run-all --parallel lint:lint lint:prettier lint:typecheck lint:spelling",
        "precommit": "npm-run-all  --parallel lint:typecheck lint:spelling --sequential fix:lint fix:prettier"
    },
    "exports": {
        "./*": "./dist/*"
    },
    "files": [
        "./dist/**/*"
    ],
    "dependencies": {
        "@sentry/tracing": "^7.93.0",
        "base64-js": "^1.5.1"
    },
    "peerDependencies": {
        "lit": "^2.8.0",
        "@sentry/browser": "^7.93.0",
        "@lit/localize": "^0.11.4"
    },
    "devDependencies": {
        "@lit/localize-tools": "^0.7.1",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "esbuild": "^0.19.10",
        "eslint": "^8.56.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-custom-elements": "0.0.8",
        "glob": "^10.3.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.1.1",
        "pseudolocale": "^2.0.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.3"
    }
}
