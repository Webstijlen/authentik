{
    "name": "@goauthentik/components",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "locales:extract-pseudo": "pseudolocalize extract -c ./lit-localize.json",
        "locales:extract": "lit-localize extract",
        "locales:build": "lit-localize build",
        "build": "npm-run-all --sequential locales:build --parallel build:compile build:types",
        "build:compile": "node ./build.mjs",
        "build:types": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.types.json",
        "clean": "rimraf dist",
        "fix:lint": "eslint . --max-warnings 0 --fix ./src/",
        "fix:prettier": "prettier --write ./src/**/* ./package.json",
        "fix": "npm-run-all --sequential fix:lint fix:prettier",
        "lint:lint": "eslint . --max-warnings 0 ./src/",
        "lint:prettier": "prettier ./src/**/* ./package.json",
        "lint:typecheck": "tsc --noEmit -p .",
        "lint:spelling": "codespell -D - -D ../../../.github/codespell-dictionary.txt -I ../../../.github/codespell-words.txt ./src -s",
        "lint": "npm-run-all --parallel lint:lint lint:prettier lint:typecheck lint:spelling",
        "precommit": "npm-run-all  --parallel lint:typecheck lint:spelling --sequential fix:lint fix:prettier",
        "fix:types": "typesync"
    },
    "exports": {
        "./*": "./dist/*"
    },
    "files": [
        "./dist/**/*"
    ],
    "dependencies": {
        "@goauthentik/common": "*",
        "@goauthentik/elements": "*",
        "@patternfly/patternfly": "^4.224.2"
    },
    "peerDependencies": {
        "@goauthentik/api": "^2023.10.6-1705263684",
        "@lit/localize": "^0.11.4",
        "lit": "^2.8.0"
    },
    "devDependencies": {
        "@lit/localize-tools": "^0.7.1",
        "@storybook/addon-essentials": "^7.6.9",
        "@storybook/addon-links": "^7.6.9",
        "@storybook/api": "^7.6.9",
        "@storybook/blocks": "^7.6.4",
        "@storybook/manager-api": "^7.6.9",
        "@storybook/web-components": "^7.6.9",
        "@storybook/web-components-vite": "^7.6.9",
        "@types/eslint": "~8.56.2",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "esbuild": "^0.19.10",
        "eslint": "^8.56.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-custom-elements": "0.0.8",
        "glob": "^10.3.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.1.1",
        "pseudolocale": "^2.0.0",
        "rimraf": "^5.0.5",
        "storybook": "^7.6.9",
        "storybook-addon-mock": "^4.3.0",
        "typescript": "^5.3.3",
        "typesync": "^0.12.1"
    }
}
